.paginator-loader
    .paginator-actions
        button(
            ng-class="meta.current_page == meta.last_page ? 'disabled' : ''"
            ng-click="loadMore()").button.button-primary {{ "paginator_loader.buttons.load" | translate }}
    div(ng-if="meta.current_page && meta.last_page").paginator-progress
        span(ng-bind="meta.current_page * meta.per_page + ' '")  
        | {{ "paginator_loader.labels.from" | translate }} 
        span(ng-bind="meta.last_page * meta.per_page")